@model AppDigitalCv.ViewModels.DocumentosVM
@using AppDigitalCv.Security

<div>

    <form id="myFormulario">
        @Html.HiddenFor(m => m.IdDocumento, new { @class = "form-control input-sm", id = "IdDocumento" })
        @Html.HiddenFor(m => m.StrUrl, new { @class = "form-control input-sm" })
        <div class="">
            <div class="form-group">
                <div class="row">
                    <div class="col-lg-3">
                        <img alt="user Pic" src="~/Imagenes/escudo_uttt.jpg" id="profile-image1" class="product-img img-responsive" width="100" height="70" />
                    </div>
                    <div class="col-lg-3">
                        ¿Deseas <strong>Eliminar</strong> este registro realmente?
                    </div>
                    <div class="col-lg-6">
                        <div class="form-group">
                            @Html.TextBoxFor(model => model.StrUrl, null, new { @class = "form-control input-sm", @disabled = "disabled" })
                        </div>
                        <div class="form-group">
                            <button class="btn btn-danger" id="btnEnviar">Eliminar</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </form>
</div>

<script>
        $(document).ready(function () {

            $("#btnEnviar").click(function () {

            $("#loaderDiv").show();

                 var myformdat = $("#myFormulario").serialize();

            $.ajax({

                type: "POST",
                url: "/digitalcv/DocumentacionPersonalV2/DeleteDocumento",
                data: myformdat,
                success: function () {
                    $("#loaderDiv").hide();
                    $("#myModal").modal("hide");
                    window.location.href = "/digitalcv/DocumentacionPersonalV2/Create";

                }

            })
        })

        })
</script>



