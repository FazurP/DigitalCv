@{
    ViewBag.Title = "Not Found Javascript";
}

<div class="error-page">
    <h2 class="headline text-red">404</h2>

    <div class="error-content">
        <h3><i class="fab fa-js text-red"></i> Se requiere JavaScript</h3>

        <p>
            Lamentamos informarte que Digital-Cv no funciona correctamente si JavaScript
            no está habilitado.
        </p>

        <a href="@Url.Action("Create", "Personal")" class="btn btn-default" role="button">Volver a Cargar</a>
        <a href="#" class="btn btn-default" role="button">Cerrar Sesion</a>
    </div>
</div>