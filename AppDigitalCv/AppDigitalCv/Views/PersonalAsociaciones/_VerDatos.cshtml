@model AppDigitalCv.ViewModels.PersonalAsociacionesVM
@using AppDigitalCv.Security

<div>
    @Html.HiddenFor(model => model.IdAsociacion, new { id = "id" })

    <div class="bs-container">

        <div class="col-md-4 col-xs-12 col-sm-6 col-lg-4">
            <img alt="user Pic" src="~/Imagenes/escudo_uttt.jpg" id="profile-image1" class="product-img img-responsive" width="100" height="70" />
        </div>
        <div class="row">
            <div class="form-group  col-lg-7">
                <ul class="todo-list">
                    <li class="list-unstyled">
                        <label>Asociación y/o Colectivo: </label> @Html.DisplayFor(model => model.Asociaciones.StrDescripcion, new { @class = "form-control input-sm", @placeholder = "Nombre", @disabled = "disabled" })
                    </li>
                    <li class="list-unstyled">
                        <label>Fecha de Ingreso: </label> @Html.DisplayFor(model => model.DteFecha, new { @class = "form-control input-sm", @placeholder = "Nombre", @disabled = "disabled" })
                    </li>
                    <li class="list-unstyled">
                        <label>Cargo que Desempeña: </label> @Html.DisplayFor(model => model.StrTipoParticipacion, new { @class = "form-control input-sm", @placeholder = "Nombre", @disabled = "disabled" })
                    </li>
                    <li class="list-unstyled">
                        <label>Función que Desempeña: </label> @Html.DisplayFor(model => model.strFuncionDesempeñada, new { @class = "form-control input-sm", @placeholder = "Nombre", @disabled = "disabled" })
                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>

