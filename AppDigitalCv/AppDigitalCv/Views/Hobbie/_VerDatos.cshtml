@model AppDigitalCv.ViewModels.HobbieVM
@using AppDigitalCv.Security
<div>
    <form id="myForm">
        <div class="bs-container">
            <div class="row">
                <div class="col-md-4 col-xs-12 col-sm-3 col-lg-3">
                    <img alt="user Pic" src="~/Imagenes/escudo_uttt.jpg" id="profile-image1" class="product-img img-responsive" width="100" height="70" />
                </div>
                <div class="col-md-9 col-xs-11 col-sm-6 col-lg-9">
                    <ul class="todo-list">
                        <li class="list-unstyled">
                            <label>Hobbie y/o Afición: </label>
                            @Html.DisplayFor(model => model.Hobbies.strValor, new { @class = "form-control input-sm", disabled = "", id = "idDeporte" })
                        </li>
                        <li class="list-unstyled">
                            <label>Frecuencia:</label>
                            @Html.DisplayFor(model => model.Frecuencia.StrDescripcion, new { @class = "form-control input-sm", disabled = "", id = "idDeporte" })
                        </li>
                        <li class="list-unstyled">
                            <label>Horas que lo Practica:</label>
                            @Html.DisplayFor(model => model.strTiempoPractica, new { @class = "form-control input-sm", disabled = "", id = "idDeporte" })
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </form>
</div>