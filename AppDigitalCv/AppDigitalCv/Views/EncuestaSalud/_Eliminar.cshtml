
@model AppDigitalCv.ViewModels.EncuestaVM
@using AppDigitalCv.Security
<div>
    <form id="myFormulario">
        @Html.HiddenFor(model => model.id, new { @class = "form-control input-sm", id = "id" })
        <div class="form-inline">
            <div class="form-group">
                <div class="row">
                    <div class="col-lg-3 col-md-3 col-sm-12">
                        <img alt="user Pic" src="~/Imagenes/escudo_uttt.jpg" id="profile-image1" class="product-img img-responsive" width="100" height="70" />
                    </div>
                    <div class="col-lg-3 col-md-3 col-sm-12">
                        ¿Deseas <strong>Eliminar</strong> este registro realmente?
                    </div>
                    <div class="col-lg-6 col-md-6 col-sm-12">
                        <ul class="todo-list">
                            <li class="list-unstyled">
                                @Html.DisplayFor(model => model.dteFechaRealizo, new { @class = "form-control input-sm", @placeholder = "Nombre", @disabled = "disabled" })

                            </li>
                        </ul>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-6 col-lg-offset-6 col-md-6 col-md-offset-6 col-sm-12">
                        <a href="#" id="btnEliminar">
                            <i class="btn btn-danger">Eliminar</i>
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </form>
</div>

<script>
    $(document).ready(function () {

        $("#btnEliminar").click(function () {

            var myformdat = $("#myFormulario").serialize();
            $.ajax({
                type: "POST",
                url: "/EncuestaSalud/DeleteEncuesta",
                data: myformdat,
                success: function () {
                    window.location.href = "/EncuestaSalud/Create";
                }
            });
        });
    });
</script>



