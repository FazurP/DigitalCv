@model AppDigitalCv.ViewModels.DeportePersonalVM
@using AppDigitalCv.Security
    <noscript>
        <meta http-equiv="refresh" content="0;URL=@Url.Action("NoScript","Error")" />
    </noscript>
<div>
    <form id="myForm">

        <div class="bs-container">
            <div class="row">
                <div class="col-md-4 col-xs-12 col-sm-3 col-lg-3">
                    <img alt="user Pic" src="~/Imagenes/escudo_uttt.jpg" id="profile-image1" class="product-img img-responsive" width="100" height="70" />
                </div>
                <div class="col-md-9 col-xs-11 col-sm-6 col-lg-9">
                    <ul class="todo-list">
                        <li class="list-unstyled">
                            <label>Deporte:</label>
                            @Html.DisplayFor(model => model.Deporte.StrDescripcion, new { @class = "form-control input-sm", disabled = "", id = "idDeporte" })
                        </li>
                        <li class="list-unstyled">
                            <label>Frecuencia:</label>
                            @Html.DisplayFor(model => model.Frecuencia.StrDescripcion, new { @class = "form-control input-sm", disabled = "", id = "idDeporte" })
                        </li>
                        <li class="list-unstyled">
                            <label>Horas que lo Practica:</label>
                            @Html.DisplayFor(model => model.strHorasPractica, new { @class = "form-control input-sm", disabled = "", id = "idDeporte" })
                        </li>
                    </ul>
                    
                </div>
            </div>

        </div>


    </form>
</div>